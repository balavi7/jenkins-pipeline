pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('build') {
            steps {
                echo 'building....building done'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploying'
            }
        }
        stage('test') {
            steps {
                echo 'testing'
            }
        }
        stage('release') {
            steps {
                echo 'releasing'
            }
        }
    }    
}    
